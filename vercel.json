{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://aryanguptasapi.vercel.app/api/:path*"
    }
  ],
  "headers": [
    {
      "source": "/:path*",
      "headers": [{ "key": "Cache-Control", "value": "no-cache" }]
    }
  ]
}